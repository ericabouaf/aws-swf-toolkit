{
    "name": "aws-swf-toolkit",
    "version": "0.1.0",
    "description": "A Node.js Framework for workflows on Amazon SWF",
    "author": "Eric Abouaf <eric.abouaf@gmail.com>",
    "preferGlobal": "true",
    "engines": {
        "node" : ">=0.8.0"
    },
    "keywords": [
        "cli",
        "amazon",
        "aws",
        "swf",
        "workflow"
    ],
    "main" : "index.js",
    "directories" : { "lib" : "./lib" },
    "repository": {
        "type":"git",
        "url":"http://github.com/neyric/aws-swf.git"
    },
    "bin": {
        "swf-activity": "./swf-activity/swf-activity.js",
        "swf-decider": "./swf-decider/swf-decider.js",
        "swf-register": "./swf-register/swf-register.js",
        "swf-set-credentials": "./swf-set-credentials/swf-set-credentials.js",
        "swf-start": "./swf-start/swf-start.js",
        "swf-decider-graph": "./swf-graph/swf-decider-graph.js"
    },
    "dependencies": {
            "aws-sdk": "0.9.1-pre.3",
            "aws-swf": "3.0.0",
            "optimist": "0.3.4",
            "async": "0.1.22",
            "colors": "0.6.0-1",
            "graphviz": "0.0.8"
    },
    "licenses" : [
        {
            "type" : "MIT",
            "url" : "https://raw.github.com/neyric/aws-swf-toolkit/master/LICENSE.txt"
        }
    ]
}